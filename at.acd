AT canyon  # checked
   ifkey look, canyon
      set status, 1
      proceed
   fin
   ifkey go
      or
   ifeq status, 1
      move entrance, in.cave
      move down, out, east, debris, debris
      move inward, up, west, bird.chamber
      move pit, pit
      ifkey canyon
         call follow.it, debris, east, bird.chamber, west
      fin
      ifeq grate, unlocked
         move depression, depression
      else
         smove depression, in.cave, cant.pass.grate
      fin
   fin
